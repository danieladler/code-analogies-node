<div id="main-content">
  {{#if stories.length}}

  {{#each stories}}
    <div class="story-wrapper">
      <p>
        {{technology}} is like {{comparison}}.
        <br>
        Here's why: {{story}}
      </p>
    </div>
  {{/each}}
  {{else }}
    <p>There is no data.</p>
  {{/if}}
</div>

<a href="/other">Go somewhere else.</a>
